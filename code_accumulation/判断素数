//#include<stdio.h>
//#include<math.h>
//
int isprime(int n)
{
	for (int i = 2; i <= sqrt(n); i++)
	{
		if (n % i == 0)
		{
			return 0;
		}
	}
	return 1;
}
int main()
{
	int num;
	scanf_s("%d", &num);
	int result = isprime(num);
	if (result== 1)
	{
		printf("%d是素数\n",num);
	}
	else if (result == 0)
	{
		printf("%d不是素数\n",num);
	}
	return 0;
}