#include<stdio.h>

void hanoi(int n, int a, int b, int c);
void move(int n, int a, int b);

int main()
{
	int n;
	scanf_s("%d", &n);
	hanoi(n, 'a', 'b', 'c');
	return 0;
}

void move(int n, int a, int b)
{
	printf("move %d from %c to %c\n", n, a, b);
}

void hanoi(int n, int a, int b, int c)
{
	if (n == 1)
	{
		move(n, a, c);
		return;
	}
	hanoi(n - 1, a, c, b);
	move(n, a, c);
	hanoi(n - 1, b, a, c);
}