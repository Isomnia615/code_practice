#include<stdio.h>

int gcd(int x, int y)
{
	int temp;
	while (y!= 0)
	{
		temp = y;
		y = x % y;
		x = temp;
	}
	return x;  //辗转相除法
}

int lcm(int m, int n)
{
	int product = m * n;
	int result = product / gcd(m, n);
	return result;
}
int main()
{
	int a, b;
	scanf_s("%d %d", &a, &b);
	int result = gcd(a, b);
	printf("a和b的最大公约数是%d\n", result);
	int result1 = lcm(a, b);
	printf("a和b的最小公倍数是%d", result1);
	return 0;
}