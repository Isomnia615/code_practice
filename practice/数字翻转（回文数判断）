#include<stdio.h>
int main()
{
	int n;
	scanf_s("%d", &n);
	int count = 0;
	for (int i = 1; i <= n; i++)
	{
		int num = i;
		int reversed = 0;
		while (num > 0)
		{
			reversed = reversed * 10 + num % 10;
			num /= 10;
		}
		if (reversed == i)
		{
			count++;
		}
	}

	printf("%d\n", count);
	return 0;
}